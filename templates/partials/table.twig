{% if table %}
    <table>
        <thead>
        <tr>
            {% for header in table.headers %}
            	<th>
                    {{ header }}
                </th>
            {% endfor %}
        </tr>
        </thead>
        <tbody>
            {% for row in table.rows %}
            	<tr>
                    {% for cell in row %}
                    	<td>
                            {{ cell }}
                        </td>
                    {% endfor %}
                </tr>
            {% endfor %}
        </tbody>
    </table>
{% endif %}